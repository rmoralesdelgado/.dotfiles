[user]
	name = Raul Morales Delgado
	email = rmoralesdelgado@gmail.com
[core]
	excludesfile = ~/.gitignore_global
	editor = code --wait
	pager = cat
[init]
	defaultBranch = main
[log]
	decorate = true
	abbrevcommit = true
[pager]
	log = false
[alias]
	nicelog = log --oneline --graph --all --decorate
[diff]
	tool = vscode
[difftool "vscode"]
	cmd = code --wait --diff $LOCAL $REMOTE
